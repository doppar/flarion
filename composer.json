{
    "name": "doppar/flarion",
    "description": "Doppar API Authentication package",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/doppar/flarion/issues",
        "source": "https://github.com/doppar/flarion"
    },
    "authors": [
        {
            "name": "Mahedi Hasan",
            "email": "mahedi@doppar.com"
        }
    ],
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Doppar\\Flarion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doppar\\Flarion\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "doppar": {
            "providers": [
                "Doppar\\Flarion\\FlarionServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": []
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
